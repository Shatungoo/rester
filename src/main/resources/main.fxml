<VBox prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.helldaisy.controllers.Main">
  <children>
      <AnchorPane fx:id="title" onMouseDragged="#mouseDrag" onMousePressed="#mousePress" styleClass="background">
         <children>
            <ToolBar prefHeight="40.0" prefWidth="157.0" AnchorPane.leftAnchor="0.0">
              <items>
                  <Button mnemonicParsing="false" onAction="#openImport" text="Import" />
                  <Button mnemonicParsing="false" onAction="#openSetings" text="Settings" />
              </items>
            </ToolBar>
            <HBox layoutX="1148.0" layoutY="-1.0" prefHeight="25.0" prefWidth="139.0" spacing="1.0" AnchorPane.rightAnchor="0.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#minimize" prefHeight="33.0" prefWidth="66.0" text="â€“" />
                  <Button mnemonicParsing="false" onAction="#maximize" prefHeight="33.0" prefWidth="66.0" text="ðŸ—–" />
                  <Button mnemonicParsing="false" onAction="#close" prefHeight="33.0" prefWidth="66.0" styleClass="close" text="Ð¥" />
               </children>
            </HBox>
         </children>
      </AnchorPane>
    <SplitPane dividerPositions="0.3" focusTraversable="true" minHeight="-Infinity" minWidth="-Infinity" VBox.vgrow="ALWAYS">
      <items>
            <TabPane SplitPane.resizableWithParent="false">
               <tabs>
                  <Tab text="History">
                     <content>
                        <ListView fx:id="history" onMouseClicked="#historySelection" prefHeight="751.0" prefWidth="175.0" />
                     </content>
                  </Tab>
                  <Tab text="Collections">
                     <content>
                        <TreeView fx:id="collections" prefHeight="200.0" prefWidth="200.0" />
                     </content>
                  </Tab>
                  <Tab text="Tests">
                     <content>
                        <TreeView fx:id="collections1" prefHeight="200.0" prefWidth="200.0" />
                     </content>
                  </Tab>
               </tabs>
            </TabPane>
            <VBox maxWidth="1.7976931348623157E308" prefHeight="751.0" prefWidth="813.0">
               <children>
                  <fx:include fx:id="requestController" source="request.fxml" />
                  <fx:include fx:id="responseController" source="response.fxml" />
               </children>
            </VBox>
      </items>
    </SplitPane>
  </children>
</VBox>
